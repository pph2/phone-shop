C9D,IAAI,OAAO,GAAG,uBAAuB,CAAC;IACtC,OAAO,KAAK,CAAC;QACX,MAAM,EAAE,MAAM;QACd,OAAO,EAAE,EAAE,cAAc,EAAE,gCAAgC,EAAE;QAC7D,OAAO;QACP,OAAO,EAAE,OAAO;QAChB,GAAG;QACH,IAAI;KACL,CAAC,CAAA;AACJ,CAAC,CAAA;AAED,eAAe,EAAE,GAAG,EAAE,IAAI,EAAE,CAAC","sourcesContent":["import axios from 'axios';\r\n\r\naxios.interceptors.request.use(\r\n  config => {\r\n    // 添加token\r\n    const token = sessionStorage.getItem('token');\r\n    const username = sessionStorage.getItem('username');\r\n    if(token && token !== 'null') {\r\n      config.headers && (config.headers['Authorization'] = token)\r\n    }\r\n    if(username && username !== 'null') {\r\n      config.headers && (config.headers['Username'] = username)\r\n    }\r\n    return config\r\n  }\r\n)\r\n\r\naxios.interceptors.response.use(\r\n  function(response) {\r\n    if(!response.data) {\r\n      return Promise.reject(response);\r\n    }\r\n    if(response.headers.token) {\r\n      sessionStorage.setItem('token', response.headers.token)\r\n    }\r\n    return response.data;\r\n  }\r\n)\r\n\r\nexport const get = (url: string, params: any, headers?: any) => {\r\n  return axios({\r\n    method: 'get',\r\n    url,\r\n    params,\r\n    headers,\r\n  })\r\n}\r\n\r\nexport const post = (url: string, data: any, timeout = 10000) => {\r\n  let backUrl = 'http://localhost:8080';\r\n  return axios({\r\n    method: 'post',\r\n    headers: { 'Content-Type': 'application/json;charset=UTF-8' },\r\n    timeout,\r\n    baseURL: backUrl,\r\n    url,\r\n    data,\r\n  })\r\n}\r\n\r\nexport default { get, post };"]}]}